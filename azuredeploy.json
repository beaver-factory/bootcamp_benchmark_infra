{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {},
  "functions": [],
  "variables": {},
  "resources": [
    {
      "name": "helloworldstorage12321",
      "type": "Microsoft.Storage/storageAccounts",
      "apiVersion": "2021-04-01",
      "tags": {
        "displayName": "helloworldstorage12321"
      },
      "location": "[resourceGroup().location]",
      "kind": "StorageV2",
      "sku": {
        "name": "Standard_LRS",
        "tier": "Standard"
      }
    },
    {
      "name": "helloworld",
      "type": "Microsoft.Web/sites",
      "apiVersion": "2020-12-01",
      "location": "[resourceGroup().location]",
      "kind": "functionapp",
      "dependsOn": [
        // "[resourceId('Microsoft.Web/serverfarms', 'serverFarmName')]",
        "[resourceId('Microsoft.Storage/storageAccounts', 'helloworldstorage12321')]"
      ],
      "properties": {
        // "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', 'serverFarmName')]",
        "siteConfig": {
          "appSettings": [
            {
              "name": "AzureWebJobsDashboard",
              "value": "[concat('DefaultEndpointsProtocol=https;AccountName=', 'helloworldstorage12321', ';AccountKey=', listKeys('storageAccountID','2015-05-01-preview').key1)]"
            },
            {
              "name": "AzureWebJobsStorage",
              "value": "[concat('DefaultEndpointsProtocol=https;AccountName=', 'helloworldstorage12321', ';AccountKey=', listKeys('storageAccountID','2015-05-01-preview').key1)]"
            },
            {
              "name": "WEBSITE_CONTENTAZUREFILECONNECTIONSTRING",
              "value": "[concat('DefaultEndpointsProtocol=https;AccountName=', 'helloworldstorage12321', ';AccountKey=', listKeys('storageAccountID','2015-05-01-preview').key1)]"
            },
            {
              "name": "WEBSITE_CONTENTSHARE",
              "value": "[toLower('functionName')]"
            },
            {
              "name": "FUNCTIONS_EXTENSION_VERSION",
              "value": "~2"
            },
            {
              "name": "APPINSIGHTS_INSTRUMENTATIONKEY",
              "value": "[reference(resourceId('microsoft.insights/components/', 'applicationInsightsName'), '2015-05-01').InstrumentationKey]"
            },
            {
              "name": "FUNCTIONS_WORKER_RUNTIME",
              "value": "python"
            }
          ]
        }
      }
    }
  ],
  "outputs": {}
}
